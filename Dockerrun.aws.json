{
    "AWSEBDockerrunVersion":2,
    "containerDefinations":[
        {
        "enviorment":[
        {
            "name":"POSTGRES_USER",
            "value":"postgres"
        },
        {
            "name":"POSTGRES_PASSWORD",
            "value":"Kaspro"
        },
        {
            "name":"POSTGRES_DB",
            "value":"nexus"
        }
    ],
    "essential":true,
    "image":"postgres",
    "memory":256,
    "name":"postgresDB",
    "portMapping":[{
        "containerPort":5432,
        "hostPort":5432
    }]
    },
    {
        "essential":true,
        "image":"redis",
        "memory":128,
        "name":"RedisStore",
        "portMapping":[{
            "containerPort":6379,
            "hostPort":6379
        }]
    },
    {
        "image":"tuser9742/serverimage:latest",
        "name":"server",
        "essential":true,
        "memory":256,
        "portMapping":[{
            "containerPort":4000,
            "hostPort":4000
        }],
        "links":[
            "postgresDB",
            "RedisStore"
        ],
        "depends_on":[
            "postgresDB",
            "RedisStore" 
        ]
       
    }
],
"volumes":[
    {
        "name":"redis_data",
        "host":{
            "sourcepath":"/var/app/current/redis_data"
        }
    },
    {
        "name":"data",
        "host":{
            "sourcepath":"/var/app/current/data"
        }
    },
    {
        "name":"server",
        "host":{
            "sourcepath":"/var/app/current/server"
        }
    }
]
}